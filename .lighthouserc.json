{
  "ci": {
    "collect": {
      "url": ["http://localhost:3000"]
    },
    "assert": {
      "assertions": {
        "categories:performance": ["warn", { "minScore": 0.9 }],
        "csp-xss": "off",
        "errors-in-console": "off",
        "installable-manifest": "off",
        "maskable-icon": "off",
        "splash-screen": "off",
        "content-width": "off"
      }
    }
  }
}

